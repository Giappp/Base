<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="chooseDateScene" prefHeight="205.0" prefWidth="523.0" xmlns="http://javafx.com/javafx/20.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.storage.SelectDate">
   <Label layoutX="188.0" layoutY="28.0" text="Choose Date Filter">
      <font>
         <Font name="Segoe UI" size="18.0"/>
      </font>
   </Label>
   <DatePicker fx:id="dpStartDate" layoutX="80.0" layoutY="91.0" promptText="Start Date"/>
   <DatePicker fx:id="dpEndDate" layoutX="315.0" layoutY="89.0" prefHeight="27.0" prefWidth="175.0"
               promptText="End Date"/>
   <Button fx:id="okBtn" layoutX="128.0" layoutY="150.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="102.0"
           style="-fx-background-color: #0091ff;" text="Ok" textFill="WHITE"/>
   <Button fx:id="cancelBtn" layoutX="315.0" layoutY="149.0" mnemonicParsing="false" prefHeight="26.0"
           prefWidth="102.0" style="-fx-background-color: transparent; -fx-border-color: black;" text="Cancel"/>
   <Label layoutX="36.0" layoutY="95.0" text="From:">
      <font>
         <Font name="Segoe UI" size="14.0"/>
      </font>
   </Label>
   <Label layoutX="287.0" layoutY="93.0" prefHeight="18.0" prefWidth="28.0" text="To:">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
</AnchorPane>
